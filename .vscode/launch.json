{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "justMyCode": false,
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python Debugger: Current Module",
      "type": "debugpy",
      "request": "launch",
      "justMyCode": false,
      "module": "${command:extension.commandvariable.file.relativeFileDotsNoExtension}",
      "args": ["--inference"],
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "LLMLingua2",
      "type": "debugpy",
      "request": "launch",
      "justMyCode": false,
      "module": "olive.workflows.run",
      "args": ["--config", "workflow_encoder.json"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/models/llm/llmlingua/",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/models/llm/llmlingua/"
      }
    },
    {
      "name": "Python Debugger: Llama QDQ",
      "type": "debugpy",
      "request": "launch",
      "justMyCode": false,
      "program": "${workspaceFolder}/models/llm/llama/main.py",
      "cwd": "${workspaceFolder}",
      "args": "${command:pickArgs}",
      "console": "integratedTerminal"
    }
  ]
}
