{
  "input_model": {
    "type": "PyTorchModel",
    "model_loader": "load_xlmroberta_tcl_encoder",
    "model_path": "microsoft/llmlingua-2-xlm-roberta-large-meetingbank",
    "model_script": "user_script.py",
    "io_config": {
      "input_names": ["hidden_states", "attention_mask"],
      "input_shapes": [
        [1, 512, 1024],
        [1, 1, 512, 512]
      ],
      "input_types": ["float32", "float32"],
      "output_names": ["last_hidden_state"],
      "output_shapes": [[1, 512, 1024]]
    }
  },
  "systems": {
    "host_system": {
      "type": "LocalSystem",
      "accelerators": [
        { "device": "cpu", "execution_providers": ["CPUExecutionProvider"] }
      ]
    },
    "target_system": {
      "type": "LocalSystem",
      "accelerators": [
        { "device": "npu", "execution_providers": ["QNNExecutionProvider"] }
      ]
    }
  },
  "engine": {
    "host": "host_system",
    "target": "target_system",
    "cache_dir": "temp/cache",
    "clean_cache": true,
    "clean_evaluation_cache": true,
    "evaluator": "common_evaluator",
    "evaluate_input_model": false,
    "output_dir": "outputs/microsoft/llmlingua-2-xlm-roberta-large-encoder"
  },
  "data_configs": [
    {
      "name": "calibration_data",
      "user_script": "user_script.py",
      "script_dir": ".",
      "load_dataset_config": {
        "type": "meetingbank_dataset",
        "data_path": "data/contexts.npz",
        "max_samples": 2,
        "model_name": "microsoft/llmlingua-2-xlm-roberta-large-meetingbank",
        "embeds_field_name": "hidden_states",
        "seq_length": 512
      },
      "dataloader_config": {
        "batch_size": 1
      }
    },
    {
      "name": "eval_data",
      "user_script": "user_script.py",
      "load_dataset_config": {
        "type": "meetingbank_dataset",
        "data_path": "data/contexts.npz",
        "max_samples": 100,
        "model_name": "microsoft/llmlingua-2-xlm-roberta-large-meetingbank",
        "embeds_field_name": "hidden_states",
        "seq_length": 512
      },
      "post_process_data_config": {
        "type": "xlmroberta_tcl_post_process"
      },
      "dataloader_config": {
        "batch_size": 1
      }
    },
    {
      "name": "TODO",
      "type": "HuggingfaceContainer",
      "user_script": "user_data.py",
      "load_dataset_config": {
        "data_name": "huuuyeah/meetingbank",
        "split": "train"
      },
      "pre_process_data_config": {
        "type": "calib_pre_process",
        "seq_length": 512,
        "model_name": "microsoft/llmlingua-2-xlm-roberta-large-meetingbank",
        "rate": 0.5
      },
      "dataloader_config": { "batch_size": 1 }
    }
  ],
  "evaluators": {
    "common_evaluator": {
      "metrics": [
        {
          "name": "quality",
          "type": "custom",
          "data_config": "eval_data",
          "sub_types": [
            {
              "name": "accuracy-r30",
              "priority": 1,
              "higher_is_better": true
            },
            { "name": "f1-r30", "higher_is_better": true },
            { "name": "accuracy-r50", "higher_is_better": true },
            { "name": "f1-r50", "higher_is_better": true },
            { "name": "accuracy-r00", "higher_is_better": true },
            { "name": "f1-r00", "higher_is_better": true }
          ],
          "user_config": {
            "user_script": "user_script.py",
            "metric_func": "eval_token_cls_with_encoder_output",
            "metric_func_kwargs": {
              "model_name": "microsoft/llmlingua-2-xlm-roberta-large-meetingbank"
            }
          }
        },
        {
          "name": "latency",
          "type": "latency",
          "data_config": "eval_data",
          "sub_types": [
            { "name": "avg" },
            { "name": "p75" },
            { "name": "p90" },
            { "name": "p99" }
          ],
          "user_config": {
            "inference_settings": {
              "onnx": {
                "execution_provider": "QNNExecutionProvider",
                "session_options": {
                  "extra_session_config": {
                    "session.disable_cpu_ep_fallback": "1"
                  }
                },
                "provider_options": [
                  {
                    "htp_graph_finalization_optimization_mode": 3,
                    "htp_performance_mode": "burst"
                  }
                ]
              }
            }
          }
        }
      ]
    }
  },
  "passes": {
    "conversion": {
      "device": "cpu",
      "type": "OnnxConversion",
      "target_opset": 17,
      "dynamic": false,
      "use_dynamo_exporter": false,
      "save_as_external_data": true,
      "all_tensors_to_one_file": true
    },
    "qnn_preprocess": {
      "type": "QNNPreprocess",
      "save_as_external_data": true,
      "all_tensors_to_one_file": true
    },
    "quantization": {
      "type": "OnnxStaticQuantization",
      "data_config": "calibration_data",
      "quant_preprocess": true,
      "activation_type": "QUInt16",
      "weight_type": "QUInt8",
      "save_as_external_data": true,
      "all_tensors_to_one_file": true
    }
  }
}
